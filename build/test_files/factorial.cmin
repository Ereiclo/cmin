
function main(){
    let x,y;

    x = 6;
    y = 1;

    while(x > 0){
        y = y*x;
        x = x - 1;
    }

    print("El factorial de 6 es: ",y,"\n");
    print("El valor de x + 1 es: ",x+1,"\n");

    let a;
    let b;

    a = 10;


    while(a > 0){
        b = 10;
        while(b > 0){
            print(a," ",b,"\n");

            b = b - 1;
        }
        a = a -1;
    }
}